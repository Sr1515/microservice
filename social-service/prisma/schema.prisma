generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id @default(uuid()) @db.Uuid
  content   String?
  imageUrl  String?
  createdAt DateTime @default(now())
  userId    String   @db.Uuid
}
